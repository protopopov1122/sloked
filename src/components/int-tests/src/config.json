{
    "containers": {
        "main": {
            "crypto": {
                "authentication": {
                    "master": {
                        "defaultUser": {
                            "restrictAccess": {
                                "content": [],
                                "whitelist": true
                            },
                            "restrictModification": {
                                "content": [],
                                "whitelist": true
                            }
                        },
                        "masterPassword": "password",
                        "salt": "salt",
                        "users": [
                            {
                                "id": "user1",
                                "password": "password1",
                                "restrictAccess": {
                                    "content": [
                                        "/document",
                                        "/namespace",
                                        "/screen",
                                        "/editor",
                                        "/plugins"
                                    ],
                                    "whitelist": true
                                },
                                "restrictModification": {
                                    "content": [
                                        "/screen",
                                        "/plugins"
                                    ],
                                    "whitelist": true
                                }
                            }
                        ]
                    }
                },
                "defaultKey": {
                    "password": "password"
                },
                "salt": "salt"
            },
            "network": {
                "buffering": 5,
                "compression": true
            },
            "parameters": {
                "tabWidth": 2
            },
            "server": {
                "netServer": {
                    "host": "localhost",
                    "port": 1234
                },
                "restrictAccess": {
                    "content": [
                        "/document",
                        "/namespace",
                        "/screen",
                        "/editor",
                        "/plugins"
                    ],
                    "whitelist": true
                },
                "restrictModification": {
                    "content": [
                        "/document",
                        "/namespace",
                        "/screen",
                        "/editor",
                        "/plugins"
                    ],
                    "whitelist": true
                },
                "services": {
                    "endpoints": [
                        "/document/render",
                        "/document/cursor",
                        "/document/manager",
                        "/document/notify",
                        "/document/search",
                        "/namespace/root",
                        "/editor/parameters",
                        "/editor/shutdown",
                        "/editor/authorization"
                    ],
                    "root": "file:///"
                }
            }
        }
    }
}